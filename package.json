{
  "name": "pic-style",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.picstyle.app",
    "productName": "Pic Style",
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "renderer.js",
      "shaders/**/*",
      "pic-styles.json"
    ],
    "extraResources": [
      {
        "from": "./cubes",
        "to": "cubes",
        "filter": [
          "**/*"
        ]
      }
    ]
  },

  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1"
  }
}
